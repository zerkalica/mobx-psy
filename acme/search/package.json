{
  "name": "@acme/search",
  "version": "1.0.0",
  "private": true,
  "description": "Example flat search application",
  "exports": {
    "./": "./-/"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf - *.log .cache",
    "build": "tsc --build && node ./-/boot/prod/build",
    "build:dev": "tsc --build && node ./-/boot/dev/build",
    "start": "bin/acme-search",
    "dev": "tsc --build && node ./-/boot/dev/server",
    "dev:node": "PSY_NO_WATCH=1 tsc-watch --build --noClear --onSuccess 'node ./-/boot/dev/server'"
  },
  "bin": {
    "acme-search": "./bin/acme-search"
  },
  "dependencies": {
    "@emotion/styled": "^11.0.0",
    "@emotion/react": "^11.0.0",
    "@psy/core": "^1.0.0",
    "@psy/react": "^1.0.0",
    "@snap/router": "^1.0.0",
    "@snap/server": "^1.0.0",
    "@snap/ui": "^1.0.0",
    "express": "^4.17.1",
    "mobx": "^6.0.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@psy/boot": "^1.0.0",
    "@snap/build": "^1.0.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.10.1",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "tsc-watch": "^4.2.9"
  },
  "peerDependencies": {
    "jest": "*",
    "typescript": "*"
  }
}
