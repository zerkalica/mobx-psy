{
    "exclude": [
        ".cache",
        "dist",
        "node_modules"
    ],
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "declaration": true,
        "strict": true,
        "module": "esnext",
        "target": "es5",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "esnext"
        ],
        "jsx": "react"
    }
}
