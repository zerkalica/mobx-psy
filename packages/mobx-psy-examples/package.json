{
  "name": "mobx-psy-examples",
  "version": "1.0.0",
  "private": true,
  "description": "Mobx pseudo syncronous examples",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/* *.tsbuildinfo",
    "build": "NODE_ENV=production tsc -b && parcel build src/browser/index.html --out-dir=dist/browser --public-url=/mobx-psy",
    "start": "NODE_ENV=production node -r source-map-support/register dist/server",
    "start:dev": "NODE_ENV=development tsc-watch --onSuccess 'node -r source-map-support/register dist/server'",
    "watch": "NODE_ENV=development tsc -b && parcel serve src/browser/index.html --open --out-dir=dist/bundle",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "express": "^4.17.1",
    "mobx": "^5.14.2",
    "mobx-psy": "^1.0.0",
    "mobx-psy-ssr": "^1.0.0",
    "node-fetch": "^2.6.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node-fetch": "^2.5.2",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "parcel": "^1.12.4",
    "source-map-support": "^0.5.13",
    "tsc-watch": "^4.0.0",
    "typescript": "3.7.1-rc"
  }
}
