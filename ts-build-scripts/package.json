{
  "name": "ts-build-scripts",
  "version": "1.0.1",
  "main": "-/index.js",
  "typings": "-/index.d.ts",
  "description": "Typescript based projects build scripts",
  "repository": "https://github.com/zerkalica/ts-build-scripts",
  "bugs": {
    "url": "https://github.com/zerkalica/ts-build-scripts/issues"
  },
  "homepage": "https://github.com/zerkalica/ts-build-scripts/tree/master/#readme",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf *.tsbuildinfo",
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "test": "jest --passWithNoTests",
    "release": "yarn build && node ./-/cli prepublish && cd - && yarn publish"
  },
  "bin": {
    "ts-build-scripts": "./bin/ts-build-scripts"
  },
  "engines": {
    "node": ">=13.2.0"
  },
  "keywords": [
    "react-scripts",
    "typescript",
    "deploy",
    "build",
    "make",
    "bundler",
    "assets"
  ],
  "dependencies": {
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.6",
    "del": "^5.1.0",
    "exits": "^2.0.1",
    "find-up": "^5.0.0",
    "globby": "^11.0.1",
    "source-map-support": "^0.5.19",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/yargs": "^15.0.5",
    "jest": "^26.4.2",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
